    "think-cache": "^1.0.0",
    "think-cache-file": "^1.0.8",
    "think-logger3": "^1.0.0",
    "think-model": "^1.4.0",
    "think-model-mysql": "^1.1.1",
    "think-router": "^1.3.4",
    "think-session": "^1.0.0",
    "think-session-file": "^1.1.3",
